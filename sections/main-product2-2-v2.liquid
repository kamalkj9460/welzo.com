{% comment %}theme-check-disable TemplateLength{% endcomment %}
{{ 'section-main-pdp2-v2.aio.min.css' | asset_url | stylesheet_tag }}
{{ 'customer.aio.min.css' | asset_url | stylesheet_tag }}
{{ 'component-accordion.aio.min.css' | asset_url | stylesheet_tag }}
{{ 'component-price.aio.min.css' | asset_url | stylesheet_tag }}
{{ 'component-rte.aio.min.css' | asset_url | stylesheet_tag }}
{{ 'component-slider.aio.min.css' | asset_url | stylesheet_tag }}
{{ 'component-rating.aio.min.css' | asset_url | stylesheet_tag }}
{{ 'component-loading-overlay.aio.min.css' | asset_url | stylesheet_tag }}

<link rel="stylesheet" href="{{ 'component-deferred-media.aio.min.css' | asset_url }}" media="print" onload="this.media='all'">

{%- style -%}
    .section-{{ section.id }}-padding {
    padding-top: {{ section.settings.padding_top | times: 0.75 | round: 0 }}px;
    padding-bottom: {{ section.settings.padding_bottom | times: 0.75 | round: 0 }}px;
    }

    @media screen and (min-width: 750px) {
    .section-{{ section.id }}-padding {
    padding-top: {{ section.settings.padding_top }}px;
    padding-bottom: {{ section.settings.padding_bottom }}px;
    }
    }
    .product2-review {
    cursor: pointer;
    }
    .product-form__input input[type=radio]:not(.disabled)+label>.visually-hidden {
    display: none;
    }
    .product-form__input input[type=radio]:disabled+label, .product-form__input input[type=radio].disabled+label {
    border-color: rgba(var(--color-foreground),.1);
    color: rgba(var(--color-foreground),.6);
    text-decoration: line-through;
    }
    .visually-hidden {
    position: absolute!important;
    overflow: hidden;
    width: 1px;
    height: 1px;
    margin: -1px;
    padding: 0;
    border: 0;
    clip: rect(0 0 0 0);
    word-wrap: normal!important;
    }
    .included-section__opener.off {
    filter: grayscale(1);
    pointer-events: none;
    }
    .product2-review {
    cursor: pointer;
    }
{%- endstyle -%}

<script src="{{ 'product-form.aio.min.js' | asset_url }}" defer="defer"></script>
<script src="{{ 'delivery-timer.aio.min.js' | asset_url }}" defer="defer"></script>

{%- assign first_3d_model = product.media | where: "media_type", "model" | first -%}
{%- if first_3d_model -%}
    {{ 'component-product-model.aio.min.css' | asset_url | stylesheet_tag }}
    <link id="ModelViewerStyle" rel="stylesheet" href="https://cdn.shopify.com/shopifycloud/model-viewer-ui/assets/v1.0/model-viewer-ui.css" media="print" onload="this.media='all'">
    <link id="ModelViewerOverride" rel="stylesheet" href="{{ 'component-model-viewer-ui.aio.min.css' | asset_url }}" media="print" onload="this.media='all'">
{%- endif -%}

{% comment %} MEDIA BLOCK {% endcomment %}

{%- capture media_block -%}
    <div class="product2__media grid__item">
        <div class="product2__slider {% if product.media.size == 1 %} hide_one{% endif %}"  id="MediaGallery-{{ section.id }}">
            <div class="product-main">
                {% for image in product.media %}
                    <div class="item">
                        <img loading="lazy"
                             srcset="{%- if image.width >= 165 -%}{{ image | img_url: '165x' }} 165w,{%- endif -%}
                    {%- if image.width >= 360 -%}{{ image | img_url: '360x' }} 360w,{%- endif -%}
                    {%- if image.width >= 533 -%}{{ image | img_url: '533x' }} 533w,{%- endif -%}
                    {%- if image.width >= 720 -%}{{ image | img_url: '720x' }} 720w,{%- endif -%}
                    {%- if image.width >= 940 -%}{{ image | img_url: '940x' }} 940w,{%- endif -%}
                    {%- if image.width >= 1066 -%}{{ image | img_url: '1066x' }} 1066w,{%- endif -%}
                    {{ image | img_url: 'master' }} {{ image.width }}w"
                             src="{{ image | img_url: '533x' }}"
                             sizes="(min-width: {{ settings.page_width }}px) {{ settings.page_width | minus: 130 | divided_by: 4 }}px, (min-width: 990px) calc((100vw - 130px) / 4), (min-width: 750px) calc((100vw - 120px) / 3), calc((100vw - 35px) / 2)"
                             alt="{{ image.alt | escape }}"
                             class="motion-reduce"
                             {% unless lazy_load == false %}loading="lazy"{% endunless %}
                             width="{{ image.width }}"
                             height="{{ image.height }}"
                        >
                    </div>
                {% endfor %}
            </div>
            <div class="product-thumb">
                {% for image in product.media %}
                    <div class="item">
                        {{ image | image_url: width: 100, height: 100, crop: center | image_tag }}
                    </div>
                {% endfor %}
            </div>
        </div>
        <script src="https://cdnjs.cloudflare.com/ajax/libs/slick-carousel/1.8.1/slick.min.js" integrity="sha512-XtmMtDEcNz2j7ekrtHvOVR4iwwaD6o/FUJe6+Zq+HgcCsk3kj4uSQQR8weQ2QVj1o0Pk6PwYLohm206ZzNfubg==" crossorigin="anonymous" referrerpolicy="no-referrer"></script>
        <link rel="stylesheet" href="https://cdnjs.cloudflare.com/ajax/libs/slick-carousel/1.8.1/slick.css" integrity="sha512-wR4oNhLBHf7smjy0K4oqzdWumd+r5/+6QO/vDda76MW5iug4PT7v86FoEkySIJft3XA0Ae6axhIvHrqwm793Nw==" crossorigin="anonymous" referrerpolicy="no-referrer" />
        <link rel="stylesheet" href="https://cdnjs.cloudflare.com/ajax/libs/slick-carousel/1.8.1/slick-theme.min.css" integrity="sha512-17EgCFERpgZKcm0j0fEq1YCJuyAWdz9KUtv1EjVuaOz8pDnh/0nZxmU6BBXwaaxqoi9PQXnRWqlcDB027hgv9A==" crossorigin="anonymous" referrerpolicy="no-referrer" />

        {% if section.settings.enable_reviews %}
            <div class="product2-reviews__sld product2__desktop">
                <div class="prod-review__slider">
                    <div class="swiper-wrapper ">
                        {% if section.settings.image_reviews != blank  and section.settings.name_reviews != blank and section.settings.text_reviews != blank %}
                            <div class="swiper-slide prod2-review__slide">
                                <div class="prod2-review__slide-box">
                                    {% if section.settings.image_reviews != blank %}
                                        <div class=" prod2-review__slide-img">
                                            <img loading="lazy" class="snippet-image__image"

                                                 srcset="{%- if section.settings.image_reviews.width >= 275 -%}{{ section.settings.image_reviews | img_url: '275x' }} 275w,{%- endif -%}
                                  {%- if section.settings.image_reviews.width >= 330 -%}{{ section.settings.image_reviews | img_url: '330x' }} 330w,{%- endif -%}
                                  {%- if section.settings.image_reviews.width >= 445 -%}{{ section.settings.image_reviews | img_url: '445x' }} 445w,{%- endif -%}
                                  {%- if section.settings.image_reviews.width >= 550 -%}{{ section.settings.image_reviews | img_url: '550x' }} 550w,{%- endif -%}
                                  {%- if section.settings.image_reviews.width >= 710 -%}{{ section.settings.image_reviews | img_url: '710x' }} 710w,{%- endif -%}
                                  {%- if section.settings.image_reviews.width >= 828 -%}{{ section.settings.image_reviews | img_url: '828x' }} 828w,{%- endif -%}
                                  {{ section.settings.image_reviews | img_url: 'master' }} {{ section.settings.image_reviews.width }}w"
                                                 src="{{ section.settings.image_reviews | img_url: '90x' }}"
                                                 alt="{{ section.settings.image_reviews.alt }}">
                                        </div>
                                    {%- endif -%}
                                    <div class="prod2-review__slide-content">
                                        <div class="prod2-review__slide-raiting">{%- render 'icon-raitingstars' %}</div>
                                        {% if section.settings.name_reviews != blank %}
                                            <div class="prod2-review__slide-title"><strong>{{ section.settings.name_reviews }}</strong> reviewed <strong>welzo.com</strong></div>
                                        {%- endif -%}
                                        {% if section.settings.text_reviews != blank %}
                                            <div class="prod2-review__slide-text">"{{ section.settings.text_reviews }}"</div>
                                        {%- endif -%}
                                    </div>
                                </div>
                            </div>
                        {%- endif -%}
                        {% if section.settings.image_reviews2 != blank  and section.settings.name_reviews2 != blank and section.settings.text_reviews2 != blank %}
                            <div class="swiper-slide prod2-review__slide">
                                <div class="prod2-review__slide-box">
                                    {% if section.settings.image_reviews2 != blank %}
                                        <div class=" prod2-review__slide-img">
                                            <img loading="lazy" class="snippet-image__image"

                                                 srcset="{%- if section.settings.image_reviews2.width >= 275 -%}{{ section.settings.image_reviews2 | img_url: '90x' }} 275w,{%- endif -%}
                                  {%- if section.settings.image_reviews2.width >= 330 -%}{{ section.settings.image_reviews2 | img_url: '90x' }} 330w,{%- endif -%}
                                  {%- if section.settings.image_reviews2.width >= 445 -%}{{ section.settings.image_reviews2 | img_url: '90x' }} 445w,{%- endif -%}
                                  {%- if section.settings.image_reviews2.width >= 550 -%}{{ section.settings.image_reviews2 | img_url: '90x' }} 550w,{%- endif -%}
                                  {%- if section.settings.image_reviews2.width >= 710 -%}{{ section.settings.image_reviews2 | img_url: '90x' }} 710w,{%- endif -%}
                                  {%- if section.settings.image_reviews2.width >= 828 -%}{{ section.settings.image_reviews2 | img_url: '90x' }} 828w,{%- endif -%}
                                  {{ section.settings.image_reviews2 | img_url: 'master' }} {{ section.settings.image_reviews2.width }}w"
                                                 src="{{ section.settings.image_reviews2 | img_url: '90x' }}"
                                                 alt="{{ section.settings.image_reviews2.alt }}">
                                        </div>
                                    {%- endif -%}
                                    <div class="prod2-review__slide-content">
                                        <div class="prod2-review__slide-raiting">{%- render 'icon-raitingstars' %}</div>
                                        {% if section.settings.name_reviews2 != blank %}
                                            <div class="prod2-review__slide-title"><strong>{{ section.settings.name_reviews2 }}</strong> reviewed <strong>welzo.com</strong></div>
                                        {%- endif -%}
                                        {% if section.settings.text_reviews2 != blank %}
                                            <div class="prod2-review__slide-text">"{{ section.settings.text_reviews2 }}"</div>
                                        {%- endif -%}
                                    </div>
                                </div>
                            </div>
                        {%- endif -%}
                        {% if section.settings.image_reviews3 != blank  and section.settings.name_reviews3 != blank and section.settings.text_reviews3 != blank %}
                            <div class="swiper-slide prod2-review__slide">
                                <div class="prod2-review__slide-box">
                                    {% if section.settings.image_reviews3 != blank %}
                                        <div class=" prod2-review__slide-img">
                                            <img loading="lazy" class="snippet-image__image"

                                                 srcset="{%- if section.settings.image_reviews3.width >= 275 -%}{{ section.settings.image_reviews3 | img_url: '90x' }} 275w,{%- endif -%}
                                  {%- if section.settings.image_reviews3.width >= 330 -%}{{ section.settings.image_reviews3 | img_url: '90x' }} 330w,{%- endif -%}
                                  {%- if section.settings.image_reviews3.width >= 445 -%}{{ section.settings.image_reviews3 | img_url: '90x' }} 445w,{%- endif -%}
                                  {%- if section.settings.image_reviews3.width >= 550 -%}{{ section.settings.image_reviews3 | img_url: '90x' }} 550w,{%- endif -%}
                                  {%- if section.settings.image_reviews3.width >= 710 -%}{{ section.settings.image_reviews3 | img_url: '90x' }} 710w,{%- endif -%}
                                  {%- if section.settings.image_reviews3.width >= 828 -%}{{ section.settings.image_reviews3 | img_url: '90x' }} 828w,{%- endif -%}
                                  {{ section.settings.image_reviews3 | img_url: 'master' }} {{ section.settings.image_reviews3.width }}w"
                                                 src="{{ section.settings.image_reviews3 | img_url: '90x' }}"
                                                 alt="{{ section.settings.image_reviews3.alt }}">
                                        </div>
                                    {%- endif -%}
                                    <div class="prod2-review__slide-content">
                                        <div class="prod2-review__slide-raiting">{%- render 'icon-raitingstars' %}</div>
                                        {% if section.settings.name_reviews3 != blank %}
                                            <div class="prod2-review__slide-title"><strong>{{ section.settings.name_reviews3 }}</strong> reviewed <strong>welzo.com</strong></div>
                                        {%- endif -%}
                                        {% if section.settings.text_reviews3 != blank %}
                                            <div class="prod2-review__slide-text">"{{ section.settings.text_reviews3 }}"</div>
                                        {%- endif -%}
                                    </div>
                                </div>
                            </div>
                        {%- endif -%}

                    </div>
                    <div class="prod-review__slider-nav">
                        {%- render 'slider-navigation-buttons', left: "wat-say-left", right: "wat-say-right" -%}
                        <div class="swiper-pagination"></div>
                    </div>
                </div>
            </div>
        {%- endif -%}
    </div>
{%- endcapture -%}

<script src="https://code.jquery.com/jquery-3.6.0.min.js"></script>
<script>
    $(document).ready(function() {
        $('.product2-review').on('click', function(event) {
            event.preventDefault(); // Предотвращаем стандартное действие ссылки

            const offsetTop = $('.jdgm-rev-widg').offset().top;

            $('html, body').animate({
                scrollTop: offsetTop
            }, 1000);
        });
    });
</script>

<section class="page-width section-{{ section.id }}-padding">
    <div class="product product--{{ section.settings.media_size }} product--{{ section.settings.gallery_layout }} grid grid--1-col {% if product.media.size > 0 %}grid--2-col-tablet{% else %}product--no-media{% endif %}">

        <!--- v2 start--->

        {% comment %} DESKTOP MEDIA BLOCK {% endcomment %}

        {{ media_block }}

        <div class="product__info-wrapper grid__item{% if settings.page_width > 1400 and section.settings.media_size == "small" %} product__info-wrapper--extra-padding{% endif %}">
            <div id="ProductInfo-{{ section.id }}" class="product__info-container product__info-container-flex{% if section.settings.enable_sticky_info %} product__info-container--sticky{% endif %} product2__container">
                {%- assign product_form_id = 'product-form-' | append: section.id -%}
                <div>
                    {%- form 'product', product, id: 'product-form-installment', class: 'installment caption-large' -%}
                        <input type="hidden" name="id" value="{{ product.selected_or_first_available_variant.id }}">
                        {{ form | payment_terms }}
                    {%- endform -%}
                </div>

                <div class="product2__heading-box">
                    <div class="product2__heading-box-left">

                        <h1 class="product2__title product2__desktop">
                            {{ product.title | escape }}
                        </h1>

                        {% comment %} DESKTOP MEDIA BLOCK {% endcomment %}

                        {{ media_block }}
                        <div class="product2_star-block">
                            {% if section.settings.enable_reviews_rat %}
                                <div class="product2-review">
                                    {%- render 'icon-raitingstars' %}
                                    <span>
                              {%- if product.metafields.accentuate.count_reviews and product.metafields.accentuate.count_reviews > '0' -%}
                                  {{ product.metafields.accentuate.count_reviews }} Reviews
                              {% else %}
                                  {{ product.title | size | times: 2 }} Reviews
                              {% endif %}
                            </span>
                                            </div>
                                        {%- endif -%}
                        </div>
                    </div>
                    <div class="product2__heading-box-right">

                        <div class="product2__price">
                            <div class="no-js-hidden" id="price-{{ section.id }}" role="status" >
                                {%- render 'price1', product: product, use_variant: true, show_badges: true, price_class: 'price--large' -%}
                            </div>
                        </div>

                        {%- if product.available %}
                            <div class="product2__desktop">
                                <div class="product2__instock">
                                    {% render 'icon-instock' %} In stock.
                                </div>
                            </div>
                        {% endif %}

                        <div class="product2_star-block mobile">
                            {% if section.settings.enable_reviews_rat %}
                                <div class="product2-review">
                                    {%- render 'icon-raitingstars' %}
                                    <span>
                                    {%- if product.metafields.accentuate.count_reviews and product.metafields.accentuate.count_reviews > '0' -%}
                                        {{ product.metafields.accentuate.count_reviews }} Reviews
                                    {% else %}
                                        {{ product.title | size | times: 2 }} Reviews
                                    {% endif %}
                                  </span>
                                </div>
                            {%- endif -%}
                        </div>

                    </div>
                </div>

                {%- if product.description != blank -%}
                    <div class="product2__description product2__desktop short">
                        <div class="mcd_description-text short">
                            <div class="mcd_description-text-small">
                                {{ product.description | truncatewords: 24 }}
                                <div><a href="#" class="mcd_description-text-open">{{ 'sections.collection_read_more.title' | t }}</a></div>
                            </div>
                            <div class="mcd_description-text-full">
                                {{ product.description }}
                            </div>
                        </div>
                    </div>
                {%- endif -%}

                {% if section.settings.enable_delivery %}
                    <div class="product2-delivery">
                        <div class="product2-delivery__icon">
                            {% render 'icon-truck' %}
                        </div>
                        <div class="product2-delivery__content">
                            <div class="product2-delivery__date">
                                Free delivery by<span id="delivery-date"> Tomorrow, April 27.</span>
                            </div>
                            <div id="delivery-time-container" class="product2-delivery__time-item">
                                Order within <span id="delivery-time" class="product2-delivery__time"></span>
                            </div>
                        </div>
                    </div>
                {%- endif -%}

                {%- unless product.has_only_default_variant -%}
                    {%- if section.settings.picker_type == 'button' -%}
                        <variant-radios class="no-js-hidden product2__variants" data-section="{{ section.id }}" data-url="{{ product.url }}">
                            {%- for option in product.options_with_values -%}
                                <fieldset class="js product-form__input">
                                    <legend class="form__label">{{ option.name }}</legend>
                                    {%- liquid
                                        assign variants_available_arr = product.variants | map: 'available'
                                        assign variants_option1_arr = product.variants | map: 'option1'
                                        assign variants_option2_arr = product.variants | map: 'option2'
                                        assign variants_option3_arr = product.variants | map: 'option3'
                                    -%}
                                    {%- for value in option.values -%}
                                        {%- liquid
                                            assign option_disabled = true

                                            for option1_name in variants_option1_arr
                                                case option.position
                                                    when 1
                                                        if variants_option1_arr[forloop.index0] == value and variants_available_arr[forloop.index0]
                                                            assign option_disabled = false
                                                        endif
                                                    when 2
                                                        if option1_name == product.selected_or_first_available_variant.option1 and variants_option2_arr[forloop.index0] == value and variants_available_arr[forloop.index0]
                                                            assign option_disabled = false
                                                        endif
                                                    when 3
                                                        if option1_name == product.selected_or_first_available_variant.option1 and variants_option2_arr[forloop.index0] == product.selected_or_first_available_variant.option2 and variants_option3_arr[forloop.index0] == value and variants_available_arr[forloop.index0]
                                                            assign option_disabled = false
                                                        endif
                                                endcase
                                            endfor
                                        -%}
                                        <input type="radio" id="{{ section.id }}-{{ option.position }}-{{ forloop.index0 }}"
                                               name="{{ option.name }}"
                                               value="{{ value | escape }}"
                                               form="{{ product_form_id }}"
                                               position="{{ option.position }}"
                                               {% if option.selected_value == value %}checked{% endif %}
                                                {% if option_disabled %}
                                                    class="disabled"
                                                {% endif %}
                                               onchange="setTimeout(() => {
                                    onVariantChange();
                                  }, 150)"
                                        >
                                        <label for="{{ section.id }}-{{ option.position }}-{{ forloop.index0 }}">
                                            {{ value }}
                                            <span class="visually-hidden">Variant sold out or unavailable</span>
                                        </label>
                                    {%- endfor -%}
                                </fieldset>
                            {%- endfor -%}
                            <script type="application/json">
                {{ product.variants | json }}
              </script>
                        </variant-radios>



                    {%- else -%}

                        <variant-selects class="no-js-hidden" data-section="{{ section.id }}" data-url="{{ product.url }}">
                            {%- for option in product.options_with_values -%}
                                <div class="product-form__input product-form__input--dropdown">
                                    <label class="form__label" for="Option-{{ section.id }}-{{ forloop.index0 }}">
                                        {{ option.name }}
                                    </label>
                                    <div class="select">
                                        <select id="Option-{{ section.id }}-{{ forloop.index0 }}"
                                                class="select__select"
                                                name="options[{{ option.name | escape }}]"
                                                form="{{ product_form_id }}"
                                        >
                                            {%- for value in option.values -%}
                                                <option value="{{ value | escape }}" {% if option.selected_value == value %}selected="selected"{% endif %}>
                                                    {{ value }}
                                                </option>
                                            {%- endfor -%}
                                        </select>
                                        {% render 'icon-caret' %}
                                    </div>
                                </div>
                            {%- endfor -%}

                            <script type="application/json">
                {{ product.variants | json }}
              </script>
                        </variant-selects>
                    {%- endif -%}
                {%- endunless -%}

                <noscript class="product-form__noscript-wrapper-{{ section.id }}">
                    <div class="product-form__input{% if product.has_only_default_variant %} hidden{% endif %}">
                        <label class="form__label" for="Variants-{{ section.id }}">{{ 'products.product.product_variants' | t }}</label>
                        <div class="select">
                            <select name="id" id="Variants-{{ section.id }}" class="select__select" form="{{ product_form_id }}">
                                {%- for variant in product.variants -%}
                                    <option
                                            {% if variant == product.selected_or_first_available_variant %}selected="selected"{% endif %}
                                        {% if variant.available == false %}disabled{% endif %}
                                            value="{{ variant.id }}"
                                    >
                                        {{ variant.title }}
                                        {%- if variant.available == false %} - {{ 'products.product.sold_out' | t }}{% endif %}
                                        - {{ variant.price | money | strip_html }}
                                    </option>
                                {%- endfor -%}
                            </select>
                            {% render 'icon-caret' %}
                        </div>
                    </div>
                </noscript>

                <div class="product2__form-block">
                    <div class="product2__form-holder">

                        <div class="product-form__input product-form__quantity" {% unless section.settings.enable_quantity %}style="display:none;"{%- endunless -%}>
                            <label class="form__label" for="Quantity-{{ section.id }}">
                                {{ 'products.product.quantity.label' | t }}
                            </label>

                            <quantity-input class="quantity">
                                <button class="quantity__button no-js-hidden" name="minus" type="button">
                                    <span class="visually-hidden">{{ 'products.product.quantity.decrease' | t: product: product.title | escape }}</span>
                                    {% render 'icon-minus' %}
                                </button>
                                <input class="quantity__input"
                                       type="number"
                                       name="quantity"
                                       id="Quantity-{{ section.id }}"
                                       min="1"
                                        {% if product.metafields.custom.max_quantity %}
                                            max="{{ product.metafields.custom.max_quantity }}"
                                        {% endif %}
                                       value="1"
                                       form="{{ product_form_id }}"
                                >
                                <button class="quantity__button no-js-hidden plus" name="plus" type="button">
                                    <span class="visually-hidden">{{ 'products.product.quantity.increase' | t: product: product.title | escape }}</span>
                                    {% render 'icon-plus' %}
                                </button>
                            </quantity-input>
                        </div>

                        <div class="product2__form {% unless section.settings.enable_quantity %} product2__form-full-width{%- endunless -%}">
                            <product-form class="product-form">
                                <div class="product-form__error-message-wrapper" role="alert" hidden>
                                    <svg aria-hidden="true" focusable="false" role="presentation" class="icon icon-error" viewBox="0 0 13 13">
