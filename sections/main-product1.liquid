                                                </div>
                                            </div>
                                        </label>
                                        {% for group in product.selling_plan_groups %}
                                            {% for selling_plan in group.selling_plans %}
                                                {% assign price_adjustments2 = selling_plan.price_adjustments | first %}
                                                {% assign value_type2 = price_adjustments2.value_type %}
                                                {% assign value2 = price_adjustments2.value %}

                                                {% assign tmp_value2 = value2 | times: 1.0 %}
                                                {% assign value_percentage2 = tmp_value2 | divided_by: 100 %}
                                                {% assign discount_price2 = money_price | times: value_percentage2 %}

