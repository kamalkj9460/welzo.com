                <defs>
                <linearGradient id="paint0_linear_134_279" x1="947.912" y1="566.679" x2="226.922" y2="149.698" gradientUnits="userSpaceOnUse">
                <stop stop-color="#AAAEFF"/>
                <stop offset="0.2" stop-color="#D2D4FF"/>
                <stop offset="0.6" stop-color="#F7F7FF"/>
                <stop offset="1" stop-color="#EBECFF"/>
                </linearGradient>
                </defs>
              </svg>
            </div>
            
            {% if section.settings.image_background != blank %}<div class="hero-section4__img-bg" style="background-color:{{ section.settings.image_background }}"></div>{%- endif -%}        
            <img class="hero-section4__img"
              src="data:image/png;base64,iVBORw0KGgoAAAANSUhEUgAAAAEAAAABCAQAAAC1HAwCAAAAC0lEQVR42mNkYAAAAAYAAjCB0C8AAAAASUVORK5CYII=" data-class="LazyLoad"
              data-srcset="{%- if section.settings.image.width >= 275 -%}{{ section.settings.image | img_url: '275x' }} 275w,{%- endif -%}
                      {%- if section.settings.image.width >= 550 -%}{{ section.settings.image | img_url: '550x' }} 550w,{%- endif -%}
                      {%- if section.settings.image.width >= 710 -%}{{ section.settings.image | img_url: '710x' }} 710w,{%- endif -%}
                      {%- if section.settings.image.width >= 1420 -%}{{ section.settings.image | img_url: '1420x' }} 1420w,{%- endif -%}
                      {{ section.settings.image | img_url: 'master' }} {{ section.settings.image.width }}w"
              data-mob-src="{{ section.settings.image | img_url: '550x' }}"
              data-src="{{ section.settings.image | img_url: '710x' }}"
              width="{{ section.settings.image.width }}"
              height="{{ section.settings.image.height }}"
              alt="{{ section.settings.image.alt }}">{% comment %}{% endcomment %}
          </div>
        </div>
      {%- endif -%}
     
      {% if section.settings.show_review %}
        <div class="hero-section4__review">
          {% if section.settings.reviews_count != blank and section.settings.reviews_text != blank %}
            <div class="hero-section4__review-left">
              <div class="hero-section4__review-left-count">
                {% render 'icon-woman' %}
                {{ section.settings.reviews_count }}
              </div>
              <div class="hero-section4__review-left-text">{{ section.settings.reviews_text }}</div>
            </div>
          {%- endif -%}
          {% if section.settings.reviews_text2 != blank and section.settings.reviews_url != blank %}
            <div class="hero-section4__review-right">
              <div class="hero-section4__review-right-text">{{ section.settings.reviews_text2 }}</div>
              <a class="hero-section4__review-right-link" href="{{ section.settings.reviews_url }}" target="_blank">
                {% render 'icon-trust-index-pilot' %}              
              </a>
            </div>
          {%- endif -%}
              <div class="hero-section4__review-right-text hero-section4__review-right--subtitle" style="margin-top: 10px; color: rgb(18, 2, 47); width: 100%;">
                {{ section.settings.reviews_subtitle }}
              </div>
        </div>
      {%- endif -%}

      {% if section.settings.show_blocks %}
        <div class="hero-section4__blocks">
          {% if section.settings.block_title1 != blank %}
            <div class="hero-section4__blocks-item">
              {% if section.settings.block_icon1 != blank %}
                <div class="hero-section4__blocks-item-icon">
                  {% if section.settings.block_icon1 == "icon-heart" %}
                    {% render 'icon-heart' %}
                  {% elsif section.settings.block_icon1 == "icon-ogpa" %}
                    {% render 'icon-ogpa' %}
                  {% else %}
                    {% render 'icon-pils' %}
                  {% endif %}
                </div>
              {% endif %}
              <h4 class="hero-section4__blocks-item-title">{{ section.settings.block_title1 }}</h4>
              {% if section.settings.block_btn_text1 != blank and section.settings.block_btn_url1 != blank %}
                <div class="hero-section4__blocks-item-link">
                  <a href="{{ section.settings.block_btn_url1 }}">{{ section.settings.block_btn_text1 }}</a>
                </div>
              {% endif %}
            </div>
          {%- endif -%}
          {% if section.settings.block_title2 != blank %}
            <div class="hero-section4__blocks-item">
              {% if section.settings.block_icon2 != blank %}
                <div class="hero-section4__blocks-item-icon">
                  {% if section.settings.block_icon2 == "icon-heart" %}
                    {% render 'icon-heart' %}
                  {% elsif section.settings.block_icon2 == "icon-ogpa" %}
                    {% render 'icon-ogpa' %}
                  {% else %}
                    {% render 'icon-pils' %}
                  {% endif %}
                </div>
              {% endif %}
              <h4 class="hero-section4__blocks-item-title">{{ section.settings.block_title2 }}</h4>
              {% if section.settings.block_btn_text2 != blank and section.settings.block_btn_url2 != blank %}
                <div class="hero-section4__blocks-item-link">
                  <a href="{{ section.settings.block_btn_url2 }}">{{ section.settings.block_btn_text2 }}</a>
                </div>
              {% endif %}
            </div>
          {%- endif -%}
          {% if section.settings.block_title3 != blank %}
            <div class="hero-section4__blocks-item">
              {% if section.settings.block_icon3 != blank %}
                <div class="hero-section4__blocks-item-icon">
                  {% if section.settings.block_icon3 == "icon-heart" %}
                    {% render 'icon-heart' %}
                  {% elsif section.settings.block_icon3 == "icon-ogpa" %}
                    {% render 'icon-ogpa' %}
                  {% else %}
                    {% render 'icon-pils' %}
                  {% endif %}
                </div>
              {% endif %}
              <h4 class="hero-section4__blocks-item-title">{{ section.settings.block_title3 }}</h4>
              {% if section.settings.block_btn_text3 != blank and section.settings.block_btn_url3 != blank %}
                <div class="hero-section4__blocks-item-link">
                  <a href="{{ section.settings.block_btn_url3 }}">{{ section.settings.block_btn_text3 }}</a>
                </div>
              {% endif %}
            </div>
          {%- endif -%}
        </div>
      {%- endif -%}

      {% if section.settings.show_logos %}
        <div class="hero-section__logos-holder">
          {% if section.settings.title_news != blank %}
            <h6 class="hero-section__logos-title">{{ section.settings.title_news }}</h6>
          {%- endif -%}
          <div class="hero-section__logos-list">
            {% if section.settings.image_news1 %}
            <div class="hero-section__logos-item">
              <img class="hero-section__logos__image"
                loading="lazy"
                src="data:image/png;base64,iVBORw0KGgoAAAANSUhEUgAAAAEAAAABCAQAAAC1HAwCAAAAC0lEQVR42mNkYAAAAAYAAjCB0C8AAAAASUVORK5CYII=" data-class="LazyLoad"
                data-srcset="{%- if section.settings.image_news1.width >= 275 -%}{{ section.settings.image_news1 | img_url: '275x' }},{%- endif -%}
                        {%- if section.settings.image_news1.width >= 550 -%}{{ section.settings.image_news1 | img_url: '550x' }},{%- endif -%}
                        {%- if section.settings.image_news1.width >= 710 -%}{{ section.settings.image_news1 | img_url: '710x' }},{%- endif -%}
                        {%- if section.settings.image_news1.width >= 1420 -%}{{ section.settings.image_news1 | img_url: '1420x' }},{%- endif -%}
                        {{ section.settings.image_news1 | img_url: 'master' }} {{ section.settings.image_news1.width }}w"
                data-src="{{ section.settings.image_news1 | img_url: '64x' }}"
                alt="{{ section.settings.image_news1.alt }}">
            </div>  
          {%- endif -%}
          {% if section.settings.image_news2 %}
