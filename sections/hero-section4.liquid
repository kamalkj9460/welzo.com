{% schema %}
{
  "name": "t:sections.hero-section.name4",
  "tag": "section",
  "class": "section",
  "settings": [
    {
      "type": "text",
      "id": "title",
      "label": "Title",
      "default": "Convenient healthcare from home"
    },
    {
      "type": "color_background",
      "id": "image_bg",
      "label": "Section background"
    },
    {
      "type": "image_picker",
      "id": "image",
      "label": "Image"
    },
    {
      "type": "color",
      "id": "image_background",
      "label": "Image Background",
      "default": "#172C85"
    },
    {
      "type": "header",
      "content": "Reviews"
    },
    {
      "type": "checkbox",
      "id": "show_review",
      "label": "Show reviews",
      "default": true
    },
    {
      "type": "text",
      "id": "reviews_count",
      "label": "Count reviews",
      "default": "475.4k"
    },
    {
      "type": "text",
      "id": "reviews_text",
      "label": "Reviews text",
      "default": "Happy customers"
    },
    {
      "type": "text",
      "id": "reviews_text2",
      "label": "Reviews text 2",
      "default": "<strong>“Excellent”</strong> based on over 24,000 reviews"
    },
